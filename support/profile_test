# ~/.profile: executed by the command interpreter for login shells.
# This file is not read by bash(1), if ~/.bash_profile or ~/.bash_login
# exists.
# see /usr/share/doc/bash/examples/startup-files for examples.
# the files are located in the bash-doc package.

# the default umask is set in /etc/profile; for setting the umask
# for ssh logins, install and configure the libpam-umask package.
#umask 022

# if running bash
if [ -n "$BASH_VERSION" ]; then
    # include .bashrc if it exists
    if [ -f "$HOME/.bashrc" ]; then
	. "$HOME/.bashrc"
    fi
fi

# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/bin" ] ; then
    PATH="$HOME/bin:$PATH"
fi

# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/.local/bin" ] ; then
    PATH="$HOME/.local/bin:$PATH"
fi

# set PATH so it includes user's Go bin folder if it exists
if [ -d "$HOME/local/go/bin" ] ; then
    PATH="$HOME/local/go/bin:$PATH"
fi

# set PATH so it includes user's Script folder if it exists
if [ -d "$HOME/scripts" ] ; then
    PATH="$HOME/scripts:$PATH"  
fi

# set PATH so it includes user's Snap folder if it exists
if [ -d "/snap/bin" ] ; then
    PATH="/snap/bin:$PATH"  
fi

export PATH=$PATH:/usr/local/go/bin
#!/bin/bash
# Version: 0.0.2
export BK='\e[0;44;30m'
export RED='\e[1;31m'
export GN='\e[1;32m'
export YW='\e[1;33m'
export BL='\e[1;34m'
export PL='\e[1;35m'
export CY='\e[1;36m'
export WT='\e[1;37m'
export WTU='\e[4;37m'
export OG='\e[1;93m'
export OGU='\e[1;93m'
export OGF='\e[0;33;44m'
export OGG='\e[0;32;44m'
export OGH='\e[0;30;44m'
export UK='\e[0;38m'
#export BG='\e[0;44m'
export NC='\e[0m'
export ITAL=$(tput sitm)
export BOLD=$(tput bold)

export EDITOR=/usr/local/bin/nano
export prompt_symbol=㉿
export prompt_color=${GN}
export info_color=${BL}
export install_compiled=$HOME/compiled
export compiled_dir=$HOME/compiled
export scripts_dir=/opt/easy-linux
export install_keyrings="/etc/apt/keyrings"
export wordlist="/usr/share/wordlists/corys-bigass-wordlist.txt"
export backup_dir=/opt/backup
export hostname=$(echo /etc/hostname)
export pwnagotchi_mac=""
export pwnagotchi_host=""
export gotcha_mac=""
export sniffer_mac=""
export adapter=""
export updates=$(apt list --upgradable 2>/dev/null | wc -l)
export security_updates=$(apt list --upgradable 2>/dev/null | grep -i security | wc -l)
export security_pct=$(echo "scale=2; ($security_updates/$updates)*100" | bc)
############################################################  Below item were added by support/.whoami.sh

export docker_installed=0
export fatrat_installed=0
export hacktool_installed=0
export hcxdump_installed=0
export nano_installed=0
export webmin_installed=0
export wifite_installed=0
export stand_install=0
export airc_installed=0
export airc_deps_installed=0
export airg_deps_inst=0
export airg_installed=0
export autoj_install=0
export user=beesoc
export username=beesoc
export useraccount=beesoc
export computername=updates
export hostname=updates
export computername=updates
export arch=x86_64
export wordlist=/usr/share/wordlists/Top304Thousand-probable-v2.txt
export amiPwn=0
export pwnagotchi="Sniffer"
export pwn_installed=0
export OS=
export VER=
export KERN=6.1.0-kali7-amd64
